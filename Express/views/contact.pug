extends base.pug

block style
    style
        include ../static/style.css
        include ../static/stylecontact.css
block content
    div.container
        h2 Contact Us
        form(action="/contact", method="post", class="myform")
            input(type="name", name="name",, id="myclass", placeholder="Enter your name")
            input(type="phone", name="phone",id="myclass", placeholder="Enter your phone")
            input(type="email", name="email",id="myclass", placeholder="Enter your email")
            input(type="text", name="address" , id="myclass",placeholder="Enter your address")
            input(type="text", name="docs", id="myclass" ,placeholder="Enter your docs")
            button(class="btn")
                |submit